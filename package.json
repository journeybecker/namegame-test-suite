{
  "name": "namegame-test-suite",
  "version": "1.0.0",
  "description": "Automated test suite for WillowTree's Name Game",
  "scripts": {
    "cy": "cypress open",
    "cy:chrome": "cypress run --headed --browser chrome",
    "cy:chromium": "cypress run --headed --browser chromium",
    "cy:firefox": "cypress run --headed --browser firefox",
    "testcafe-chrome": "testcafe chrome testcafe/tests",
    "testcafe-safari": "testcafe safari testcafe/tests",
    "testcafe-firefox": "testcafe firefox testcafe/tests",
    "testcafe-edge": "testcafe edge testcafe/tests"
  },
  "devDependencies": {
    "cypress": "^14.1.0",
    "testcafe": "^3.7.2"
  },
  "dependencies": {
    "@cypress/xvfb": "^1.2.4",
    "xvfb": "^0.4.0"
  }
}
